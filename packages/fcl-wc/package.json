{
  "name": "@onflow/fcl-wc",
  "version": "6.0.11",
  "description": "WalletConnect adapter for FCL",
  "license": "Apache-2.0",
  "author": "Flow Foundation",
  "homepage": "https://flow.com",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/onflow/fcl-js.git"
  },
  "bugs": {
    "url": "https://github.com/onflow/fcl-js/issues"
  },
  "files": [
    "dist",
    "types",
    "react-native.js"
  ],
  "source": {
    "./src/index.ts": {
      "cjs": "./dist/index.js",
      "esm": "./dist/index.module.mjs"
    },
    "./src/react-native.ts": {
      "cjs": "./dist/react-native.js",
      "esm": "./dist/react-native.module.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.module.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.module.mjs",
      "require": "./dist/index.js"
    },
    "./react-native": {
      "types": "./types/react-native.d.ts",
      "import": "./dist/react-native.module.mjs",
      "require": "./dist/react-native.js",
      "default": "./dist/react-native.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "jest",
    "build": "fcl-bundle",
    "test:watch": "jest --watch",
    "start": "fcl-bundle --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/preset-typescript": "^7.25.7",
    "@onflow/fcl-bundle": "1.7.1",
    "@onflow/typedefs": "1.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^46.10.1",
    "jest": "^29.7.0",
    "jest-preset-preact": "^4.1.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.7",
    "@onflow/config": "1.6.3",
    "@onflow/util-invariant": "1.2.5",
    "@onflow/util-logger": "1.3.4",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/types": "^2.20.2",
    "@walletconnect/universal-provider": "^2.20.2",
    "@walletconnect/utils": "^2.20.2",
    "postcss-cli": "^11.0.0",
    "preact": "^10.24.3",
    "tailwindcss": "^3.4.14"
  },
  "peerDependencies": {
    "@onflow/fcl-core": "1.22.3",
    "@walletconnect/sign-client": "^2.20.2",
    "expo-linking": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "@walletconnect/sign-client": {
      "optional": true
    },
    "expo-linking": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
