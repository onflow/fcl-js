{
  "name": "@onflow/kit",
  "version": "0.0.1-kit.0",
  "description": "React library for interacting with the Flow blockchain",
  "license": "Apache-2.0",
  "author": "Flow Foundation",
  "homepage": "https://www.flow.com",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/onflow/fcl-js.git"
  },
  "bugs": {
    "url": "https://github.com/onflow/fcl-js/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.module.js",
      "require": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./provider": {
      "import": "./dist/provider/index.module.js",
      "require": "./dist/provider/index.js",
      "types": "./types/provider/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.module.js",
      "require": "./dist/hooks/index.js",
      "types": "./types/hooks/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.module.js",
      "require": "./dist/components/index.js",
      "types": "./types/components/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "types/index.d.ts",
  "source": "src/index.ts",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "jest",
    "build": "fcl-bundle",
    "test:watch": "jest --watch",
    "start": "fcl-bundle --watch"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.25.7",
    "@onflow/fcl-bundle": "1.6.0",
    "@onflow/typedefs": "^1.4.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsdoc": "^46.10.1",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.25.7",
    "@tanstack/react-query": "^5.67.3",
    "@testing-library/react": "^16.2.0"
  },
  "peerDependencies": {
    "@onflow/fcl": "1.13.5",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
